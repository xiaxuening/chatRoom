#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/bin/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/stylus@0.54.8/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules:/Users/xiaxuening/Desktop/node_modules:/Users/xiaxuening/node_modules:/Users/node_modules:/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/stylus-loader@3.0.2_stylus@0.54.8/node_modules/stylus-loader/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/stylus-loader@3.0.2_stylus@0.54.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/bin/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/stylus@0.54.8/node_modules/stylus/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/stylus@0.54.8/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules:/Users/xiaxuening/Desktop/node_modules:/Users/xiaxuening/node_modules:/Users/node_modules:/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/stylus-loader@3.0.2_stylus@0.54.8/node_modules/stylus-loader/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/stylus-loader@3.0.2_stylus@0.54.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylus@0.54.8/node_modules/stylus/bin/stylus" "$@"
else
  exec node  "$basedir/../../../../../stylus@0.54.8/node_modules/stylus/bin/stylus" "$@"
fi
