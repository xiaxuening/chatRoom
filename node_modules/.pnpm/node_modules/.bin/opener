#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/opener@1.5.2/node_modules/opener/bin/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/opener@1.5.2/node_modules/opener/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/opener@1.5.2/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules:/Users/xiaxuening/Desktop/node_modules:/Users/xiaxuening/node_modules:/Users/node_modules:/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/node_modules/opener/bin/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/node_modules/opener/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/opener@1.5.2/node_modules/opener/bin/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/opener@1.5.2/node_modules/opener/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/opener@1.5.2/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules:/Users/xiaxuening/Desktop/node_modules:/Users/xiaxuening/node_modules:/Users/node_modules:/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/node_modules/opener/bin/node_modules:/Users/xiaxuening/Desktop/chatRoom/node_modules/.pnpm/node_modules/opener/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../opener/bin/opener-bin.js" "$@"
else
  exec node  "$basedir/../opener/bin/opener-bin.js" "$@"
fi
