  width 100%\n\n          .progress-item\n            width 200px\n            position absolute\n            top 0\n            left 0\n            bottom 0\n            right 0\n            border-radius 10px\n            background linear-gradient(270deg,#4493d7,#fff)\n\n        .lyric\n          padding 20px\n\n          p\n            color: #aaa\n            padding-top 8px\n\n        .control\n          .el-button-group\n            width 100%\n            display flex\n\n          .el-button\n            flex 1\n\n          .iconfont\n            font-size 30px\n\n          .icon-zanting, .icon-bofang\n            font-size 34px\n          .icon-suiji\n            font-size 26px\n\n      .tool\n        width 100%\n\n        .el-button-group\n          width 100%\n          display flex\n          padding-left 10px\n\n          .el-button\n            padding 0 5px\n\n\n    .chat-box\n      flex 1\n      display flex\n      flex-direction column\n\n      .chat-input\n        height 150px\n        border-top 1px solid #333\n        display flex\n        flex-direction column\n\n        .chat-input-tool\n          height 30px\n          padding-left 10px\n\n          .el-button\n            color #fff\n\n            &:hover\n              color #409eff\n\n            .iconfont\n              margin-right 4px\n\n        .chat-input-value\n          flex 1\n          // padding 0px 10px\n\n          .el-textarea\n            height 100%\n\n\n      .chat-content\n        flex 1\n</style>\n<style lang=\"stylus\">\n.chat-input\n  .chat-input-value\n    .el-textarea__inner\n      height 100%\n      border none\n      border-radius inherit\n      background transparent\n      color #fff\n      resize none\n      outline none\n\n</style>\n"],"sourceRoot":""}]}